{
  "name": "sofo-task",
  "version": "1.3.0",
  "description": "Task management system.",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "make build-app && NODE_ENV=development node build/server.js",
    "migration": "ts-node ./src/migration/migration.ts",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "unit": "jest -c jest.config.unit.js --coverage --detectOpenHandles --forceExit",
    "integration": "jest -c jest.config.integration.js --coverage --detectOpenHandles --forceExit",
    "commit": "git-cz",
    "release": "standard-version",
    "lint": "eslint --ext .ts ./ --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "typings": "./src/type.d.ts",
  "repository": "https://github.com/AlfieriChou/sofo-task",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/bcrypt": "3.0.0",
    "@types/bunyan": "1.8.6",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.3",
    "@types/koa": "2.0.49",
    "@types/koa-generic-session": "1.0.3",
    "@types/koa-jwt": "3.3.0",
    "@types/koa-logger": "3.1.1",
    "@types/koa-redis": "3.0.3",
    "@types/koa-router": "7.0.42",
    "@types/moment": "2.13.0",
    "@types/node": "12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "3.0.5",
    "jest": "^25.0.0",
    "lint-staged": "^9.2.5",
    "nodemon": "1.19.2",
    "openapi3-ts": "1.3.0",
    "standard-version": "7.0.0",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@trapts/schedule": "1.0.3",
    "bcrypt": "3.0.6",
    "bunyan": "1.8.12",
    "bunyan-rotating-file-stream": "1.6.3",
    "chalk": "2.4.2",
    "class-transformer": "0.2.3",
    "class-transformer-validator": "0.7.1",
    "class-validator": "0.10.0",
    "dir_filenames": "1.0.6",
    "error-stack-parser": "2.0.4",
    "jaeger-client": "3.16.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.19.4",
    "koa": "2.8.1",
    "koa-bodyparser": "4.2.1",
    "koa-generic-session": "2.0.1",
    "koa-jwt": "3.6.0",
    "koa-logger": "3.2.1",
    "koa-redis": "4.0.0",
    "koa-router": "7.4.0",
    "koa-views": "6.2.1",
    "moment": "2.24.0",
    "mysql": "2.17.1",
    "nunjucks": "3.2.0",
    "opentracing": "0.14.4",
    "reflect-metadata": "0.1.13",
    "typedi": "0.8.0"
  }
}
