{
  "name": "sofo-task",
  "version": "1.0.0",
  "description": "Task management system.",
  "scripts": {
    "start": "node build/server.js",
    "format": "prettier --write \"**/*.ts\"",
    "migration": "ts-node ./src/migration/migration.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --coverage --detectOpenHandles --forceExit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "typings": "./src/type.d.ts",
  "repository": "https://github.com/AlfieriChou/sofo-task",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "private": true,
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/knex": "0.16.1",
    "@types/koa": "2.0.48",
    "@types/koa-logger": "3.1.1",
    "@types/koa-router": "7.0.40",
    "@types/node": "11.13.0",
    "husky": "1.3.1",
    "jest": "24.6.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.1",
    "ts-node": "8.0.3",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "@trapts/schedule": "1.0.3",
    "dir_filenames": "1.0.6",
    "knex": "0.16.3",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-logger": "3.2.0",
    "koa-router": "7.4.0",
    "mysql": "2.16.0"
  }
}
