{
  "name": "sofo-task",
  "version": "1.3.0",
  "description": "Task management system.",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "make build-app && NODE_ENV=development node build/server.js",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "migration": "ts-node ./src/migration/migration.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "unit": "jest -c jest.config.unit.js --coverage --detectOpenHandles --forceExit",
    "integration": "jest -c jest.config.integration.js --coverage --detectOpenHandles --forceExit",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "typings": "./src/type.d.ts",
  "repository": "https://github.com/AlfieriChou/sofo-task",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@types/bcrypt": "3.0.0",
    "@types/bunyan": "1.8.6",
    "@types/jest": "24.0.13",
    "@types/jsonwebtoken": "8.3.2",
    "@types/koa": "2.0.48",
    "@types/koa-generic-session": "1.0.2",
    "@types/koa-jwt": "3.3.0",
    "@types/koa-logger": "3.1.1",
    "@types/koa-redis": "3.0.2",
    "@types/koa-router": "7.0.40",
    "@types/moment": "2.13.0",
    "@types/node": "11.13.12",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "openapi3-ts": "1.3.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "standard-version": "6.0.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@trapts/schedule": "1.0.3",
    "bcrypt": "3.0.6",
    "bunyan": "1.8.12",
    "bunyan-rotating-file-stream": "1.6.3",
    "chalk": "2.4.2",
    "class-transformer": "0.2.3",
    "class-transformer-validator": "0.7.1",
    "class-validator": "0.9.1",
    "dir_filenames": "1.0.6",
    "error-stack-parser": "2.0.2",
    "jaeger-client": "3.15.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.17.0",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-generic-session": "2.0.1",
    "koa-jwt": "3.5.1",
    "koa-logger": "3.2.0",
    "koa-redis": "4.0.0",
    "koa-router": "7.4.0",
    "koa-views": "6.2.0",
    "moment": "2.24.0",
    "mysql": "2.17.1",
    "nunjucks": "3.2.0",
    "opentracing": "0.14.3",
    "reflect-metadata": "0.1.13",
    "typedi": "0.8.0"
  }
}
