{
  "name": "sofo-task",
  "version": "1.1.0",
  "description": "Task management system.",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "NODE_ENV=development node build/server.js",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "migration": "ts-node ./src/migration/migration.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "typings": "./src/type.d.ts",
  "repository": "https://github.com/AlfieriChou/sofo-task",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/jest": "24.0.11",
    "@types/knex": "0.16.1",
    "@types/koa": "2.0.48",
    "@types/koa-generic-session": "1.0.2",
    "@types/koa-logger": "3.1.1",
    "@types/koa-redis": "3.0.2",
    "@types/koa-router": "7.0.40",
    "@types/node": "11.13.0",
    "commitizen": "3.0.7",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.3.1",
    "jest": "24.6.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "standard-version": "5.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.1",
    "ts-node": "8.0.3",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "@trapts/schedule": "1.0.3",
    "class-transformer": "0.2.0",
    "class-transformer-validator": "0.7.1",
    "dir_filenames": "1.0.6",
    "knex": "0.16.3",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-generic-session": "2.0.1",
    "koa-logger": "3.2.0",
    "koa-redis": "3.1.3",
    "koa-router": "7.4.0",
    "mysql": "2.16.0",
    "reflect-metadata": "0.1.13"
  }
}
